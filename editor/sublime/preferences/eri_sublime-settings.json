{
	"color_scheme": "Packages/User/SublimeLinter/Solarized (Light) (SL).tmTheme",
	"font_size": 12,
	"ignored_packages":
	[
		"Vintage"
	],
	"on_save":
	[
		{
			"args":
			{
				"focus_view": false,
				"run":
				[
					"sh",
					"go build . errors && go test -i && go test"
				]
			},
			"cmd": "gs9o_open"
		}
	],
	"show_encoding": true,
	"word_wrap": "true",
	"tab_size": 4,
	"translate_tabs_to_spaces": true,
	"trim_trailing_white_space_on_save": true
}
